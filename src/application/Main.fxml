<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141" fx:controller="application.MainController">
   <children>
      <LineChart fx:id="lineChart" layoutX="50.0" layoutY="66.0" title="LINE CHART">
        <xAxis>
          <CategoryAxis label="My X Label" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis label="My Y Label" side="LEFT" />
        </yAxis>
      </LineChart>
      <Button fx:id="loadChart" layoutX="244.0" layoutY="512.0" mnemonicParsing="false" onAction="#btn" text="Load Chart">
         <font>
            <Font name="Britannic Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
